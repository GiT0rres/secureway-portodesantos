{
  "expo": {
    "name": "SecureWay",
    "slug": "SecureWay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/truck.png",
    "scheme": "secureway",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#083044",
        "foregroundImage": "./assets/images/truck.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.motakky.secureWay"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/truck.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#083044",
          "dark": {
            "backgroundColor": "#083044"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "812d1c8d-b9e6-472a-87d6-dc9a67f4b3ec"
      }
    }
  }
}
